import empleados.*

	describe "test empleados" {
		test "500 empanadas vendidas" {	
		baigorria.vendio(500)	
		assert.equals(7500,baigorria.sueldo())
		}
		
		test "sueldo galvan"{
			galvan.sueldo(20000)
			assert.equals(20000,galvan.sueldo())
			
		}
		
		test "300 empanadas vendidas" {	
		baigorria.vendio(300)	
		assert.that(true)
		}
		
		test "jefe paga baigorria" {
			jefe.pagar(baigorria)
			assert.that(true)
		}
		
		test "pago galvan" {
			jefe.pagar(galvan)
			assert.equals(15000,galvan.sueldo())
		}
}